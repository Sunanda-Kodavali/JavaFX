<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="dci.j24e01.pokemon_bynumber.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <HBox fx:id="hBox" alignment="TOP_CENTER">
        <TextField fx:id="field" prefWidth="100"/>
        <Button fx:id="button1"  text="Hello!" onAction="#onHelloButtonClick"/>
    </HBox>
    <HBox fx:id="hBox1" alignment="TOP_CENTER">
    <Label fx:id="welcomeText"/>
    <ImageView fx:id="frontShinyImage">

    </ImageView>
    </HBox>

</VBox>
